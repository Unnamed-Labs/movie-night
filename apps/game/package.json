{
  "name": "@movie/game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules test-results",
    "dev": "pnpm with-env next dev",
    "lint": "cross-env dotenv -v SKIP_ENV_VALIDATION=true next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "cross-env dotenv -e ../../.env --",
    "test:playwright": "playwright test",
    "test:dev": "cross-env start-server-and-test dev 3000 test:playwright",
    "test:start": "cross-env start-server-and-test start 3000 test:playwright",
    "test": "cross-env PLAYWRIGHT_HEADLESS=true start-server-and-test start 3000 test:playwright"
  },
  "dependencies": {
    "@movie/api": "^0.1.0",
    "@movie/auth": "^0.1.0",
    "@movie/ui": "^0.1.0",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.44.1",
    "@trpc/next": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "next": "^13.2.4",
    "next-auth": "4.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@movie/tailwind-config": "0.1.0",
    "@movie/tsconfig": "0.1.0",
    "@playwright/test": "^1.37.1",
    "@types/node": "^18.15.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "postcss": "^8.4.24",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.0"
  }
}
